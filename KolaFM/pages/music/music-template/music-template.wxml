<template name="musicTemplate">
  <view class=" articlelist ">
    <view class="author-time ">
      <image class="authoricon " src="{{authoricon}}"></image>
      <text class="author-name ">{{author}}</text>
      <text class="time ">{{time}}</text>
    </view>
    <text class="title ">{{title}}</text>
    <image class="articleimg " src="{{articleimg}}"></image>
    <text class="articletext ">{{articletext}}</text>
    <view class="article-like ">
      <image class="article-like-icon " src="../images/sc.png "></image>
      <text class="article-like-text ">{{like_start}}</text>
      <image class="article-like-icon collect_icon" src="../images/fx.png "></image>
      <text class="article-like-text ">{{like_num}}</text>
    </view>
  </view>
</template>